{
  "name": "unillm-ts-monorepo",
  "version": "1.0.0",
  "description": "Unified TypeScript library for multiple LLM providers - Monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter './packages/*' run build",
    "build:core": "pnpm --filter @unillm-ts/core run build",
    "build:server": "pnpm --filter @unillm-ts/server run build",
    "clean": "pnpm -r --filter './packages/*' run clean",
    "dev:server": "pnpm --filter @unillm-ts/server run dev",
    "start:server": "pnpm --filter @unillm-ts/server run start",
    "examples:core": "pnpm --filter @unillm-ts/core run examples:basic",
    "test": "echo 'Tests coming soon...' && exit 0"
  },
  "keywords": [
    "llm",
    "monorepo",
    "typescript",
    "openai",
    "qwen",
    "zhipu",
    "moonshot",
    "spark"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
