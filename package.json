{
  "name": "unillm-ts",
  "version": "1.0.0",
  "description": "Unified TypeScript library for multiple LLM providers (OpenAI, Qwen, ZhiPu, Moonshot, Spark)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "dev": "tsc --watch --project tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example:basic": "ts-node examples/basic.ts",
    "example:multi": "ts-node examples/multi-model.ts",
    "example:conversation": "ts-node examples/conversation.ts",
    "setup:keys": "ts-node examples/setup-keys.ts"
  },
  "keywords": [
    "llm",
    "openai",
    "qwen",
    "zhipu",
    "moonshot",
    "kimi",
    "spark",
    "chatgpt",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "undici-types": "^6.0.0"
  },
  "files": [
    "dist",
    "src/config/models.json",
    "src/config/templates.json"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

