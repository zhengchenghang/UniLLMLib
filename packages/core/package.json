{
  "name": "@unillm-ts/core",
  "version": "1.0.0",
  "description": "Unified TypeScript library for multiple LLM providers (OpenAI, Qwen, ZhiPu, Moonshot, Spark)",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package": "./package.json"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:pro": "cross-env NODE_ENV=production rollup -c",
    "build": "npm run build:pro",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "examples:setup": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" examples/setup-keys.ts",
    "examples:basic": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" examples/basic.ts",
    "examples:conversation": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" examples/conversation.ts",
    "examples:multi-model": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" examples/multi-model.ts"
  },
  "keywords": [
    "llm",
    "openai",
    "qwen",
    "zhipu",
    "moonshot",
    "kimi",
    "spark",
    "chatgpt",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "keytar": "^7.9.0",
    "react-native-keychain": "^8.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.5",
    "postcss": "^8.4.35",
    "rollup": "^4.25.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.6.3",
    "undici-types": "^6.0.0"
  },
  "files": [
    "dist/",
    "src/config/models.json",
    "src/config/templates.json"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
